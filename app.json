{
  "expo": {
    "name": "FolklorTrack",
    "slug": "FolklorTrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.carlosemmanuel.FolklorTrack"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceso a tus fotos para que puedas seleccionar una imagen para la región.",
          "cameraPermission": "La app necesita acceso a tu cámara si deseas tomar una foto para la región."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "17e90951-eae1-4d68-9fde-9d1aa5b77d1c"
      }
    }
  }
}
